{
    "name": "nzmebooks/craft-state-helper",
    "description": "A simple Craft CMS plugin that provides the ability to submit information which can be stored against a user account, and later retrieved.",
    "type": "craft-plugin",
    "version": "2.1.5",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft-state-helper"
    ],
    "support": {
        "docs": "https://github.com/nzmebooks/craft-state-helper/blob/master/README.md",
        "issues": "https://github.com/nzmebooks/craft-state-helper/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "meBooks",
            "homepage": "https://mebooks.co.nz"
        }
    ],
    "require": {
        "craftcms/cms": "^4.4",
        "php": "^8.0.2"
    },
    "autoload": {
        "psr-4": {
          "nzmebooks\\statehelper\\": "src/"
        }
    },
    "extra": {
        "name": "State Helper",
        "handle": "statehelper",
        "hasCpSettings": false,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/nzmebooks/craft-state-helper/master/CHANGELOG.md",
        "components": {
            "statehelperService": "nzmebooks\\statehelper\\services\\StatehelperService"
        },
        "developer": "Jason Darwin",
        "developerUrl": "https://mebooks.co.nz",
        "class": "nzmebooks\\statehelper\\Statehelper"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    }
}
